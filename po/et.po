msgctxt "score-dialog"
msgid "Score"
msgstr "Tulemus"

#. Score format for time based scores.  %1$d is the time in minutes, %2$d is the time in seconds
#, c-format
msgctxt "score-dialog"
msgid "%1$dm %2$ds"
msgstr "%1$dm %2$ds"

msgid "New Game"
msgstr "Uus mäng"

#. Score dialog column header for the name of the player who recorded the score
msgctxt "score-dialog"
msgid "Name"
msgstr "Nimi"

